<Window x:Class="SDKDemo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SDKDemo"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <DockPanel>
            <StackPanel Orientation="Horizontal" DockPanel.Dock="Top">
                <Button Name="btnInit" Content="Init" Margin="2" Click="btnInit_Click"/>
                <Button Name="btnClose" Content="Close" Margin="2" Click="btnClose_Click"/>
                <Button Name="btnDestory" Content="Destory" Margin="2" Click="btnDestory_Click"/>
            </StackPanel>

            <Grid>
                
            </Grid>
        </DockPanel>

    </Grid>
</Window>
